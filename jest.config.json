{
  "testEnvironment": "node",
  "collectCoverageFrom": [
    "src/*.js",
    "!**/__tests__/**/*.js"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "<rootDir>/build",
    "<rootDir>/coverage"
  ],
  "modulePathIgnorePatterns": [
    "<rootDir>/build"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 100,
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  },
  "testRegex": "/__tests__/(.*).test.jsx?$"
}
